{
    "name": "nexus/timebank",
    "description": "Project NEXUS Timebanking Platform",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Nexus\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "chillerlan/php-qrcode": "^5.0",
        "endroid/qr-code": "^6.0",
        "graham-campbell/result-type": "v1.1.3",
        "guzzlehttp/guzzle": "7.10.0",
        "guzzlehttp/promises": "2.3.0",
        "guzzlehttp/psr7": "2.8.0",
        "minishlink/web-push": "^10.0",
        "nikic/php-parser": "v5.7.0",
        "phpoption/phpoption": "1.9.4",
        "psr/http-client": "1.0.3",
        "psr/http-factory": "1.1.0",
        "psr/http-message": "2.0",
        "pusher/pusher-php-server": "^7.2",
        "ralouphie/getallheaders": "3.0.3",
        "spomky-labs/otphp": "^11.4",
        "symfony/deprecation-contracts": "v3.6.0",
        "symfony/polyfill-ctype": "v1.33.0",
        "symfony/polyfill-mbstring": "v1.33.0",
        "symfony/polyfill-php80": "v1.33.0",
        "sentry/sentry": "^4.0",
        "vlucas/phpdotenv": "v5.6.2"
    },
    "require-dev": {
        "myclabs/deep-copy": "1.13.4",
        "phar-io/manifest": "2.0.4",
        "phar-io/version": "3.2.1",
        "phpunit/php-code-coverage": "10.1.16",
        "phpunit/php-file-iterator": "4.1.0",
        "phpunit/php-invoker": "4.0.0",
        "phpunit/php-text-template": "3.0.1",
        "phpunit/php-timer": "6.0.0",
        "phpunit/phpunit": "^10.5.62",
        "sebastian/cli-parser": "2.0.1",
        "sebastian/code-unit": "2.0.0",
        "sebastian/code-unit-reverse-lookup": "3.0.0",
        "sebastian/comparator": "^5.0.5",
        "sebastian/complexity": "3.2.0",
        "sebastian/diff": "5.1.1",
        "sebastian/environment": "6.1.0",
        "sebastian/exporter": "5.1.4",
        "sebastian/global-state": "6.0.2",
        "sebastian/lines-of-code": "2.0.2",
        "sebastian/object-enumerator": "5.0.0",
        "sebastian/object-reflector": "3.0.0",
        "sebastian/recursion-context": "5.0.1",
        "sebastian/type": "4.0.0",
        "sebastian/version": "4.0.1",
        "theseer/tokenizer": "1.3.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "suggest": {
        "ext-redis": "For Redis caching support",
        "ext-imagick": "For advanced image processing",
        "ext-memcached": "For Memcached caching support",
        "ext-sodium": "For modern cryptography support"
    }
}
