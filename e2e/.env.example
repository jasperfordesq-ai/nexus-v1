# E2E Test Environment Configuration
# Copy this file to .env.test and fill in the values

# Base URL for the test environment
E2E_BASE_URL=http://localhost:5173

# React frontend URL (origin used for localStorage)
E2E_REACT_URL=http://localhost:5173

# API base URL for auth calls
E2E_API_URL=http://localhost:8090

# Default tenant slug for tests
E2E_TENANT=hour-timebank

# Test user credentials (React SPA)
E2E_USER_EMAIL=test@hour-timebank.ie
E2E_USER_PASSWORD=TestPassword123!

# Admin user credentials
E2E_ADMIN_EMAIL=admin@hour-timebank.ie
E2E_ADMIN_PASSWORD=AdminPassword123!
